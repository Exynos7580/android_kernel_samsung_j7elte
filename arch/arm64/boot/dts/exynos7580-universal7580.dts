/dts-v1/;
#include "exynos7580-reserve-2000MB.dtsi"
#include "exynos7580.dtsi"
#include "modem-carmen2-pdata.dtsi"
#include "exynos7580-carmen2_battery.dtsi"
#include "exynos7580-carmen2_fingerprint-sensor_00.dtsi"

&pinctrl_0 {
	pinctrl-names = "default","sleep";
	pinctrl-0 = <&initial0>;
	pinctrl-1 = <&sleep0>;
	initial0: initial-state {
		gpa1-0 {
			samsung,pins = "gpa1-0"; /* SSP_AP_INT */
			samsung,pin-function = <1>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
			samsung,pin-val = <1>;
		};
		gpa1-5 {
			samsung,pins = "gpa1-5"; /* SSP_MCU_INT1 */
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		gpa1-6 {
			samsung,pins = "gpa1-6"; /* SSP_MCU_INT2 */
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	sleep0: sleep-state {
		gpa1-0 {
			samsung,pins = "gpa1-0"; /* SSP_AP_INT */
			samsung,pin-con-pdn = <3>;
			samsung,pin-pud-pdn = <0>;
		};
		gpa1-5 {
			samsung,pins = "gpa1-5"; /* SSP_MCU_INT1 */
			samsung,pin-con-pdn = <3>;
			samsung,pin-pud-pdn = <0>;
		};
	};
};

&pinctrl_6 {
	pinctrl-names = "default";
	pinctrl-0 = <&initial6>;
	initial6: initial-state {
		gpf1-4 {
			samsung,pins = "gpf1-4"; /* SSP_RST */
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
			samsung,pin-val = <1>;
		};
	};
};

/ {
	model = "Samsung Universal7580 board based on EXYNOS7580";
	model_info-chip = <7580>;
	model_info-platform = "android";
	model_info-subtype = "samsung";
	model_info-hw_rev = <0>;
	model_info-hw_rev_end = <0>;
	compatible = "samsung,exynos7580-universal7580", "samsung,exynos7580";

	chosen {
		bootargs = "console=ttySAC1,115200n8 ess_setup=0x46000000 pmic_info=0x02";
		linux,initrd-start = <0x42000000>;
		linux,initrd-end = <0x421FFFFF>;
	};

	fixed-rate-clocks {
		oscclk {
			compatible = "samsung,exynos7580-oscclk";
			clock-frequency = <26000000>;
		};
	};

	dwmmc0@13540000 {
		status = "okay";
		num-slots = <1>;
		broken-cd;
		use-smu;
		fixed_volt;
		clock-gate;
		enable-cclk-on-suspend;
		only_once_tune;
		use-fine-tuning;
		ignore-phase = <(1 << 7)>;
		bypass-for-allpass;
		supports-highspeed;
		supports-hs200-mode;
		supports-ddr200-mode;
		supports-ddr200-enhanced-strobe;
		enable-cache-control;
		use-enable-shift;
		enable-ulp-mode;
		qos_int_level = <200000>;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 0>;
		samsung,dw-mshc-ddr-timing = <3 0 2 2>;
		samsung,dw-mshc-hs200-timing = <3 0 3 0>;
		samsung,dw-mshc-ddr200-timing = <1 0 2 0>;
		samsung,dw-mshc-ddr200-ulp-timing = <3 0 3 0>;
		samsung,dw-mshc-ddr200-delay-line = <0x28>;
		samsung,dw-mshc-ddr200-tx-delay-line = <0x28>;
		samsung,dw-mshc-txdt-crc-timer-fastlimit = <0x34>;
		samsung,dw-mshc-txdt-crc-timer-initval = <0x38>;
		clk_pin = "gpr0-0";
		clk_addr = "13750000.pinctrl";
		clk_val = <0x3>;
		clk_str_num = <6>;
                tp_mon_depth = <2>;
                              /* Byte/s,  MIF clk, CPU clk, KFC clk*/
                tp_mon_table = <8000000 825000  800000  0
                                0       0       0       0>;
		num-ref-clks = <10>;
		ciu_clkin = <25 50 50 100 200 100 200 400 400 200>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_rdqs &sd0_bus1 &sd0_bus4 &sd0_bus8>;
		slot@0 {
			reg = <0>;
			bus-width = <8>;
		};
	};

	dwmmc1@13550000 {
		status = "okay";
		num-slots = <1>;
		channel = <1>;
		fixed_volt;
		clock-gate;
		enable-cclk-on-suspend;
		/* broken-cd; */
		caps-control;
		supports-highspeed;
		supports-sdr104-mode;
		keep-power-in-suspend;
		pm-ignore-notify;
		cd-type-external;
		use-broken-voltage;
		use-fine-tuning;
		ignore-phase = <(1 << 7)>;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		qos_int_level = <468000>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 2 1>;
		clk_pin = "gpr2-0";
		clk_addr = "13750000.pinctrl";
		clk_val = <0x2>;
		clk_str_num = <6>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		tp_mon_depth = <2>;
				/* Byte/s,  MIF clk, CPU clk, KFC clk*/
                tp_mon_table = <8000000 825000  800000  0
				0       0       0       0>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd1_clk &sd1_cmd &sd1_bus1 &sd1_bus4>;
		slot@0 {
			reg = <0>;
			bus-width = <4>;
		};
	};

	dwmmc2@13560000 {
		status = "okay";
		num-slots = <1>;
		supports-highspeed;
		supports-sdr104-mode;
		enable-cclk-on-suspend;
		cd-type = <1>;
		bypass-smu;
		use-fine-tuning;
		clock-gate;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		qos_int_level = <468000>;
		bypass-for-allpass;
		fix-fmp-size-mismatch;
		not-allow-single-dma;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 3 2>;
		samsung,dw-mshc-ddr-timing = <3 0 2 1>;
		clk_pin = "gpr4-0";
		clk_addr = "10E60000.pinctrl";
		clk_val = <0x2>;
		clk_str_num = <6>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd2_clk &sd2_cmd &sd2_cd &sd2_bus1 &sd2_bus4 &dwmmc2_cd_ext_irq>;
		gpio-controller;
		#gpio-cells = <2>;
		cd-gpio = <&gpa3 4 0xf>;
		sec-sd-slot-type = <1>; /* Normal SD tray */
		slot@0 {
			reg = <0>;
			bus-width = <4>;
			disable-wp;
		};
	};

	gpio_keys{
		compatible = "gpio-keys";
		pinctrl-names = "default";
		pinctrl-0 = <&gpa2_pull_up_univ>;

		button@1 {
			label = "gpio-keys: KEY_POWER";
			interrupt-parent = <&gpa2>;
			linux,code = <116>;
			gpios = <&gpa2 7 0xf>;
			gpio-key,wakeup = <1>;
		};
		button@2 {
			label = "gpio-keys: KEY_VOLUMEDOWN";
			interrupt-parent = <&gpa2>;
			linux,code = <114>;
			gpios = <&gpa2 1 0xf>;
		};
		button@3 {
			label = "gpio-keys: KEY_VOLUMEUP";
			interrupt-parent = <&gpa2>;
			linux,code = <115>;
			gpios = <&gpa2 0 0xf>;
		};
		button@4 {
			label = "gpio-keys: KEY_HOMEPAGE";
			interrupt-parent = <&gpa2>;
			linux,code = <172>;
			gpios = <&gpa2 5 0xf>;
			gpio-key,wakeup = <1>;
		};
	};

	pinctrl@139B0000 {
		cfg_irq: cfg-irq {
			samsung,pins = "gpa0-3";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
		cfg_input: cfg-input {
			samsung,pins = "gpa0-3";
			samsung,pin-function = <0>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <0>;
		};
		attn_irq: attn-irq {
			samsung,pins = "gpa3-2";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		attn_input: attn-input {
			samsung,pins = "gpa3-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <0>;
		};
		if_irq: if-irq {
			samsung,pins = "gpa0-5";
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};
		muic_irq: muic-irq {
			samsung,pins = "gpa1-3";
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		dwmmc2_cd_ext_irq: dwmmc2_cd_ext_irq {
			samsung,pins = "gpa3-4";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
	};

	spi_0: spi@13910000 {
		/delete-property/ dma-mode;
		/delete-property/ dmas;
		/delete-property/ dma-names;
		#address-cells = <1>;
		#size-cells = <0>;
		pinctrl-names = "default";
		pinctrl-0 = <&spi0_bus>;
		pinctrl-1 = <&spi0_cs>;
		num-cs = <1>;
		status = "okay";

		STM32F@0 {
			compatible = "ssp,STM32F";
			reg = <0>;
			spi-max-frequency = <5000000>;
			spi-cpol;
			spi-cpha;
			pinctrl-names = "default";
			pinctrl-0 = <&ssp_rst &ssp_ap_int &ssp_mcu_int1 &ssp_mcu_int2>;

			gpio-controller;
			#gpio-cells = <2>;
			ssp,mcu_int1-gpio = <&gpa1 5 0x00>;
			ssp,mcu_int2-gpio = <&gpa1 6 0x00>;
			ssp,ap_int-gpio = <&gpa1 0 0x00>;
			ssp,rst-gpio = <&gpf1 4 0x00>;
			ssp,acc-position = <6>;
			ssp,mag-position = <7>;
			ssp,prox-hi_thresh = <130>;
			ssp,prox-low_thresh = <90>;

			controller-data {
				cs-gpio = <&gpb2 1 0>;
				samsung,spi-feedback-delay = <0>;
			};
		};
	};

	spi_1: spi@13920000 {
		/delete-property/ dma-mode;
		/delete-property/ dmas;
		/delete-property/ dma-names;
		#address-cells = <1>;
		#size-cells = <0>;
		pinctrl-names = "default";
		pinctrl-0 = <&spi1_bus>;
		pinctrl-1 = <&spi1_cs>;
		status = "okay";

		irled@0 {
			compatible = "nochip,irled";
			reg = <0>;
			spi-max-frequency = <5000000>;
			irled,default_hz = <38000>;
			irled,high_bit_size = <6>;
			irled,power_type = "ALW";
			irled,regulator = "VLED_3P3";

			controller-data {
				cs-gpio = <&gpb2 7 0>;
				samsung,spi-feedback-delay = <0>;
				samsung,spi-chip-select-mode = <0>;
			};
		};
	};

	pinctrl@138C0000 {
		gpio_cp_reset: gpio_cp_reset {
			samsung,pins = "gpc3-3";
			samsung,pin-function = <0x1>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
			samsung,pin-val = <1>;
			samsung,pin-con-pdn = <1>;
		};
	};

	pinctrl@138D0000 {
		i2c_pdn_off: i2c-pdn_off {
			samsung,pins = "gpc4-1", "gpc4-0";
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <1>;
		};
	};

	i2c@13830000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		status = "okay";

		s2801x: s2801x@7a {
			compatible = "samsung,s2801x";
			reg = <0x7a>;
			clocks = <&clock 154>,	/* sclk_audmixer */
				 <&clock 155>,	/* sclk_audmixer_bclk0*/
				 <&clock 156>,	/* sclk_audmixer_bclk1 */
				 <&clock 157>,	/* sclk_audmixer_bclk2 */
				 <&clock 152>,	/* bclk_mi2s */
				 <&clock 149>;	/* dout_audmixer*/

			clock-names = "audmixer_sysclk", "audmixer_bclk0",
					"audmixer_bclk1", "audmixer_bclk2",
					"mi2s_bclk", "audmixer_dout";
			sysreg-reset = <0x110F2000>;
			sysreg-i2c = <0x110F3000>;
			pinctrl-names = "default", "idle", "bt-idle";
			pinctrl-0 = <&i2s_pmic_bus
					&i2s_codec_clk
					&pcm_bt_bus>;
			pinctrl-1 = <&i2s_pmic_bus_idle
					&i2s_codec_clk_idle
					&pcm_bt_bus_idle>;
			pinctrl-2 = <&i2s_pmic_bus
					&i2s_codec_clk
					&pcm_bt_bus_idle>;
			samsung,lpass-subip;
		};
	};

	i2c@13860000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		status = "okay";

		touchscreen@20 {
			compatible = "synaptics,rmi4";
			reg = <0x20>;
			pinctrl-names = "on_state", "off_state";
			pinctrl-0 = <&attn_irq>;
			pinctrl-1 = <&attn_input &i2c_pdn_off>;
			synaptics,irq_gpio = <&gpa3 2 0>;
			synaptics,irq_type = <8200>;
			synaptics,max_coords = <1079 1919>;	/* x y */
			synaptics,num_lines = <29 16>;		/* rx tx */
			synaptics,regulator_dvdd = "lcd_tsp_vdd";
			synaptics,regulator_avdd = "tsp_avdd";
			synaptics,project_name = "K", "G900H";
			synaptics,firmware_name = "tsp_synaptics/synaptics_s5100_a3_k_FHD.fw";
		};
	};

	pinctrl@139C0000 {
		/* touchkey i2c*/
		cfg_i2c: cfg-i2c {
			samsung,pins = "gpc5-3", "gpc5-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
		cfg_gpio: cfg-gpio {
			samsung,pins = "gpc5-3", "gpc5-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	i2c@20 {
		compatible = "i2c-gpio";
		gpios = <&gpc5 2 0
				&gpc5 3 0>;
		i2c-gpio,delay-us = <5>;
		pinctrl-names = "default","on_i2c","off_i2c";
		pinctrl-0 = <&cfg_i2c>;
		pinctrl-1 = <&cfg_i2c>;
		pinctrl-2 = <&cfg_gpio>;
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";

		touchkey@20 {
			pinctrl-names = "on_irq","off_irq";
			pinctrl-0 = <&cfg_irq>;
			pinctrl-1 = <&cfg_input>;
			compatible = "coreriver,tc300-keypad";
			reg = <0x20>;
			interrupts = <2 0 0>;
			interrupt-parent = <&gpa0>;
			coreriver,scl-gpio = <&gpc5 3 0>;
			coreriver,sda-gpio = <&gpc5 2 0>;
			coreriver,irq-gpio = <&gpa0 3 0>;
			coreriver,regulator_ic = "vtouch_1.8v";
			coreriver,fw_name = "coreriver/tc305k_carmen2.fw";
			coreriver,sensing_ch_num = <0x4>;
			coreriver,tsk_ic_num = <0x1>; /* tc300k : 0x00, tc350k : 0x01 */
			coreriver,use_bitmap = <1>;
		};
	};

	ion {
		compatible = "samsung,exynos5430-ion";
	};

	decon_fb@14830000 {
		/* XEINT7, GPIO & pin control */
		gpios = <&gpd0 7 0xf>;
		psr_mode = <2>;		/* 0: video mode, 1: DP command mode, 2: MIPI command mode */
		disp-pll-clk = <142000000>;
		status = "okay";
	};

	dsim_0: dsim@14800000 {
		lcd_info = <&s6e3fa2>;
		decon_board = <&decon_board_list>;
		/* lcd reset, power */
		/* gpios = <&gpd1 3 0x1>, <&gpd1 1 0x1>; */
		/* lcd_vdd = "lcd_tsp_vdd"; */
		status = "okay";
	};

	decon_board_list: decon_board_list {
		dsim_set_panel_power_enable {
			type = "regulator,enable", "gpio,high", "delay,usleep";
			gpios = <&gpd1 1 0x1>;
			delay = <10000 11000>;
			regulator = "lcd_tsp_vdd";
		};
		dsim_set_panel_power_disable {
			type = "gpio,low", "delay,usleep", "gpio,low", "delay,usleep", "regulator,disable";
			gpios = <&gpd1 3 0x1>, <&gpd3 1 0x1>;
			delay = <10000 11000>, <5000 6000>;
			regulator = "lcd_tsp_vdd";
		};
		dsim_reset_panel {
			type = "gpio,high", "delay,usleep", "gpio,low", "delay,usleep", "gpio,high", "delay,usleep";
			gpios = <&gpd1 3 0x1>, <&gpd1 3 0x1>, <&gpd1 3 0x1>;
			delay = <10000 11000>, <10000 11000>, <10000 11000>;
		};
	};

	s6e3fa2: s6e3fa2_fhd {
		mode = <2>;
		resolution = <1080 1920>;
		size = <70 121>;
		timing,refresh = <60>;
		timing,h-porch = <1 1 1>;
		timing,v-porch = <12 1 1>;
		timing,dsi-hs-clk = <1024>;
		timing,dsi-escape-clk = <16>;
		mic = <0>;
	};

	usb_notifier {
		compatible = "samsung,usb-notifier";
	};

	i2c@25 {
		compatible = "i2c-gpio";
		gpios = <&gpz3 1 0
			&gpz3 0 0>;
		i2c-gpio,delay-us = <5>;
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";
		muic-s2mm001@25 {
			compatible = "sec-muic,i2c";
			reg = <0x25>;
			pinctrl-names = "default";
			pinctrl-0 = <&muic_irq>;
			muic,muic_int = <&gpa1 3 0>;
			gpios = <&gpd2 2 0>;
		};
	};

	i2c@26 {
		compatible = "i2c-gpio";
		gpios = <&gpb0 2 0
			&gpb0 3 0>;
		i2c-gpio,delay-us = <5>;
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";
		ktd2026@26 {
			compatible = "ktd2026,i2c";
			reg = <0x30>;
			pinctrl-names = "default";

			led_conf@1 {
				led_conf-name = "led_r";
				brightness = <0>;
				max_brightness = /bits/ 8 <0x20>;
				flags = <0>;
			};
			led_conf@2 {
				led_conf-name = "led_g";
				brightness = <0>;
				max_brightness = /bits/ 8 <0x20>;
				flags = <0>;
			};
			led_conf@3 {
				led_conf-name = "led_b";
				brightness = <0>;
				max_brightness = /bits/ 8 <0x20>;
				flags = <0>;
			};
		};
	};

	pinctrl@139B0000 {
		pmic_irq: pmic-irq {
			samsung,pins = "gpa0-2";
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};
		cod3022_irq: cod3022-irq {
                        samsung,pins = "gpa0-4";
                        samsung,pin-pud = <3>;
                        samsung,pin-drv = <3>;
                };
		ssp_ap_int: ssp-ap-int {
			samsung,pins = "gpa1-0";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};
		ssp_mcu_int1: ssp-mcu-int1 {
			samsung,pins = "gpa1-5";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
