/dts-v1/;
#include "exynos7580-reserve-2000MB.dtsi"
#include "exynos7580.dtsi"
#include "modem-carmen2-pdata.dtsi"
#include "exynos7580-carmen2_battery_rev01.dtsi"
#include "exynos7580-carmen2_gpio_04.dtsi"
#include "exynos7580-carmen2_fingerprint-sensor_01.dtsi"

/ {
	model = "Samsung Universal7580 revision 01 board based on EXYNOS7580";
	model_info-chip = <7580>;
	model_info-platform = "android";
	model_info-subtype = "samsung";
	model_info-hw_rev = <4>;
	model_info-hw_rev_end = <255>;
	compatible = "samsung,exynos7580-universal7580,r01", "samsung,exynos7580";

	chosen {
		bootargs = "console=ttySAC1,115200n8 ess_setup=0x46000000 pmic_info=0x02";
		linux,initrd-start = <0x42000000>;
		linux,initrd-end = <0x421FFFFF>;
	};

	fixed-rate-clocks {
		oscclk {
			compatible = "samsung,exynos7580-oscclk";
			clock-frequency = <26000000>;
		};
	};

	dwmmc0@13540000 {
		status = "okay";
		num-slots = <1>;
		broken-cd;
		use-smu;
		fixed_volt;
		clock-gate;
		enable-cclk-on-suspend;
		only_once_tune;
		use-fine-tuning;
		ignore-phase = <(1 << 7)>;
		bypass-for-allpass;
		supports-highspeed;
		supports-hs200-mode;
		supports-ddr200-enhanced-strobe;
		supports-ddr200-mode;
		enable-cache-control;
		use-enable-shift;
		qos_int_level = <200000>;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 3 2>;
		samsung,dw-mshc-hs200-timing = <3 0 3 1>;
		samsung,dw-mshc-ddr200-timing = <1 0 2 2>;
		samsung,dw-mshc-ddr200-delay-line = <118>;
		clk_pin = "gpr0-0";
		clk_addr = "13750000.pinctrl";
		clk_val = <0x3>;
		clk_str_num = <6>;
                tp_mon_depth = <2>;
                              /* Byte/s,  MIF clk, CPU clk, KFC clk*/
                tp_mon_table = <8000000 825000  800000  0
                                0       0       0       0>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_rdqs &sd0_bus1 &sd0_bus4 &sd0_bus8>;
		slot@0 {
			reg = <0>;
			bus-width = <8>;
		};
	};

	dwmmc1@13550000 {
		status = "okay";
		num-slots = <1>;
		channel = <1>;
		fixed_volt;
		clock-gate;
		enable-cclk-on-suspend;
		/* broken-cd; */
		caps-control;
		supports-highspeed;
		supports-sdr104-mode;
		keep-power-in-suspend;
		pm-ignore-notify;
		cd-type-external;
		use-broken-voltage;
		use-fine-tuning;
		ignore-phase = <(1 << 7)>;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		qos_int_level = <468000>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 2 1>;
		clk_pin = "gpr2-0";
		clk_addr = "13750000.pinctrl";
		clk_val = <0x2>;
		clk_str_num = <6>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		tp_mon_depth = <2>;
				/* Byte/s,  MIF clk, CPU clk, KFC clk*/
                tp_mon_table = <8000000 825000  800000  0
				0       0       0       0>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd1_clk &sd1_cmd &sd1_bus1 &sd1_bus4>;
		slot@0 {
			reg = <0>;
			bus-width = <4>;
		};
	};

	dwmmc2@13560000 {
		status = "okay";
		num-slots = <1>;
		supports-highspeed;
		supports-sdr104-mode;
		enable-cclk-on-suspend;
		cd-type = <1>;
		bypass-smu;
		use-fine-tuning;
		clock-gate;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		qos_int_level = <468000>;
		bypass-for-allpass;
		fix-fmp-size-mismatch;
		not-allow-single-dma;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 3 2>;
		samsung,dw-mshc-ddr-timing = <3 0 2 1>;
		clk_pin = "gpr4-0";
		clk_addr = "10E60000.pinctrl";
		clk_val = <0x2>;
		clk_str_num = <6>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd2_clk &sd2_cmd &sd2_cd &sd2_bus1 &sd2_bus4 &dwmmc2_cd_ext_irq>;
		gpio-controller;
		#gpio-cells = <2>;
		cd-gpio = <&gpa3 4 0xf>;
		sec-sd-slot-type = <1>; /* Normal SD tray */
		slot@0 {
			reg = <0>;
			bus-width = <4>;
			disable-wp;
		};
	};

	gpio_keys{
		compatible = "gpio-keys";
		pinctrl-names = "default";
		pinctrl-0 = <&gpa2_pull_up_univ>;

		button@1 {
			label = "gpio-keys: KEY_POWER";
			interrupt-parent = <&gpa2>;
			linux,code = <116>;
			gpios = <&gpa2 7 0xf>;
			gpio-key,wakeup = <1>;
		};
		button@2 {
			label = "gpio-keys: KEY_VOLUMEDOWN";
			interrupt-parent = <&gpa2>;
			linux,code = <114>;
			gpios = <&gpa2 1 0xf>;
		};
		button@3 {
			label = "gpio-keys: KEY_VOLUMEUP";
			interrupt-parent = <&gpa2>;
			linux,code = <115>;
			gpios = <&gpa2 0 0xf>;
		};
		button@4 {
			label = "gpio-keys: KEY_HOMEPAGE";
			interrupt-parent = <&gpa2>;
			linux,code = <172>;
			gpios = <&gpa2 5 0xf>;
			gpio-key,wakeup = <1>;
		};
	};

	pinctrl@138D0000 {
		i2c_pdn_off: i2c-pdn_off {
			samsung,pins = "gpc4-1", "gpc4-0";
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <1>;
		};
	};

	pinctrl@139B0000 {
		pmic_irq: pmic-irq {
			samsung,pins = "gpa0-2";
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};
		cfg_irq: cfg-irq {
			samsung,pins = "gpa0-3";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
		cfg_input: cfg-input {
			samsung,pins = "gpa0-3";
			samsung,pin-function = <0>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <0>;
		};
		cod3022_irq: cod3022-irq {
			samsung,pins = "gpa0-4";
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};
		if_irq: if-irq {
			samsung,pins = "gpa0-5";
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};
		muic_irq: muic-irq {
			samsung,pins = "gpa1-3";
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		dbmd_sensory: dbmd-sensory {
			samsung,pins = "gpa3-0";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
		};
		attn_input: attn-input {
			samsung,pins = "gpa3-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <0>;
		};
		attn_irq: attn-irq {
			samsung,pins = "gpa3-2";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		dwmmc2_cd_ext_irq: dwmmc2_cd_ext_irq {
			samsung,pins = "gpa3-4";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
		ssp_ap_int: ssp-ap-int {
			samsung,pins = "gpa1-0";
			samsung,pin-function = <1>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};
		ssp_mcu_int1: ssp-mcu-int1 {
			samsung,pins = "gpa1-5";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		ssp_mcu_int2: ssp-mcu-int2 {
			samsung,pins = "gpa1-6";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	pinctrl@139C0000 {
		vibetonz_pwm: vibetonz_pwm {
			samsung,pins = "gpc1-1";
			samsung,pin-function = <2>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		cfg_i2c: cfg-i2c {
			samsung,pins = "gpc5-3", "gpc5-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
		cfg_gpio: cfg-gpio {
			samsung,pins = "gpc5-3", "gpc5-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		sub_mic_bias_en:sub-mic-bias-en {
			samsung,pins ="gpd2-3";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-con-pdn = <3>;
			samsung,pin-pud-pdn = <0>;
			samsung,pin-val = <0>;
		};
		motor_en: motor_en {
			samsung,pins = "gpe0-7";
			samsung,pin-function = <1>;	/* output */
			samsung,pin-pud = <3>;		/* pull up */
			samsung,pin-drv = <0>;
		};
		/* Warm reset information from AP */
		pm_wrsti: pm-wrsti {
			samsung,pins = "gpe1-0";
			samsung,pin-con-pdn = <3>;
		};
		dbmd_reset: dbmd-reset {
			samsung,pins ="gpf1-6";
			samsung,pin-function = <1>;
			samsung,pin-pud = <1>;
			samsung,pin-con-pdn =<3>;
			samsung,pin-pud-pdn = <1>;
			samsung,pin-val = <0>;
		};
		dbmd_wakeup: dbmd-wakeup {
			samsung,pins ="gpf1-7";
			samsung,pin-function = <1>;
			samsung,pin-pud = <1>;
			samsung,pin-con-pdn =<3>;
			samsung,pin-pud-pdn = <1>;
			samsung,pin-val = <0>;
		};
		ssp_rst: ssp-rst {
			samsung,pins = "gpf1-4";
			samsung,pin-function = <1>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};
	};

	spi_0: spi@13910000 {
		/delete-property/ dma-mode;
		/delete-property/ dmas;
		/delete-property/ dma-names;
		#address-cells = <1>;
		#size-cells = <0>;
		pinctrl-names = "default";
		pinctrl-0 = <&spi0_bus>;
		pinctrl-1 = <&spi0_cs>;
		num-cs = <1>;
		status = "okay";

		STM32F@0 {
			compatible = "ssp,STM32F";
			reg = <0>;
			spi-max-frequency = <5000000>;
			spi-cpol;
			spi-cpha;
			pinctrl-names = "default";
			pinctrl-0 = <&ssp_rst &ssp_ap_int &ssp_mcu_int1 &ssp_mcu_int2>;

			gpio-controller;
			#gpio-cells = <2>;
			ssp,mcu_int1-gpio = <&gpa1 5 0x00>;
			ssp,mcu_int2-gpio = <&gpa1 6 0x00>;
			ssp,ap_int-gpio = <&gpa1 0 0x01>;
			ssp,rst-gpio = <&gpf1 4 0x01>;
			ssp,acc-position = <7>;
			ssp,mag-position = <7>;
			ssp,prox-hi_thresh = <130>;
			ssp,prox-low_thresh = <90>;

			controller-data {
				cs-gpio = <&gpb2 1 0>;
				samsung,spi-feedback-delay = <0>;
			};
		};
	};

	spi_1: spi@13920000 {
		/delete-property/ dma-mode;
		/delete-property/ dmas;
		/delete-property/ dma-names;
		#address-cells = <1>;
		#size-cells = <0>;
		pinctrl-names = "default";
		pinctrl-0 = <&spi1_bus>;
		pinctrl-1 = <&spi1_cs>;
		status = "okay";

		irled@0 {
			compatible = "nochip,irled";
			reg = <0>;
			spi-max-frequency = <5000000>;
			irled,default_hz = <38000>;
			irled,high_bit_size = <6>;
			irled,power_type = "REG";
			irled,regulator = "VLED_3P3";

			controller-data {
				cs-gpio = <&gpb2 7 0>;
				samsung,spi-feedback-delay = <0>;
				samsung,spi-chip-select-mode = <0>;
			};
		};
	};

	i2c@13830000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		status = "okay";
		#address-cells = <1>;
		#size-cells = <0>;

		s2801x: s2801x@7a {
			compatible = "samsung,s2801x";
			reg = <0x7a>;
			clocks = <&clock 154>,	/* sclk_audmixer */
				 <&clock 155>,	/* sclk_audmixer_bclk0*/
				 <&clock 156>,	/* sclk_audmixer_bclk1 */
				 <&clock 157>,	/* sclk_audmixer_bclk2 */
				 <&clock 152>,	/* bclk_mi2s */
				 <&clock 149>;	/* dout_audmixer*/

			clock-names = "audmixer_sysclk", "audmixer_bclk0",
					"audmixer_bclk1", "audmixer_bclk2",
					"mi2s_bclk", "audmixer_dout";
			sysreg-reset = <0x110F2000>;
			sysreg-i2c = <0x110F3000>;
			pinctrl-names = "default", "idle";
			pinctrl-0 = <&i2s_pmic_bus
					&i2s_codec_clk
					&pcm_bt_bus>;
			pinctrl-1 = <&i2s_pmic_bus_idle
					&i2s_codec_clk_idle
					&pcm_bt_bus_idle>;
			samsung,lpass-subip;
		};
	};

	i2c@13860000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		status = "okay";
		#address-cells = <1>;
		#size-cells = <0>;

		mms_ts@48 {
			compatible = "melfas,mms_ts";
			reg = <0x48>;
			pinctrl-names = "on_state", "off_state";
			pinctrl-0 = <&attn_irq>;
			pinctrl-1 = <&attn_input &i2c_pdn_off>;
			melfas,vdd_en = "tsp_avdd";
			melfas,io_en = "tsp_io";
			melfas,irq-gpio = <&gpa3 2 0>;
			melfas,scl-gpio = <&gpc4 1 0>;
			melfas,sda-gpio = <&gpc4 0 0>;
			melfas,max_x = <1080>;
			melfas,max_y = <1920>;
		};
	};

	i2c@20 {
		compatible = "i2c-gpio";
		gpios = <&gpc5 2 0
				&gpc5 3 0>;
		i2c-gpio,delay-us = <5>;
		pinctrl-names = "default","on_i2c","off_i2c";
		pinctrl-0 = <&cfg_i2c>;
		pinctrl-1 = <&cfg_i2c>;
		pinctrl-2 = <&cfg_gpio>;
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";

		touchkey@20 {
			pinctrl-names = "on_irq","off_irq";
			pinctrl-0 = <&cfg_irq>;
			pinctrl-1 = <&cfg_input>;
			compatible = "coreriver,tc300-keypad";
			reg = <0x20>;
			interrupts = <2 0 0>;
			interrupt-parent = <&gpa0>;
			coreriver,scl-gpio = <&gpc5 3 0>;
			coreriver,sda-gpio = <&gpc5 2 0>;
			coreriver,irq-gpio = <&gpa0 3 0>;
			coreriver,regulator_ic = "vtouch_1.8v";
			coreriver,fw_name = "coreriver/tc305k_carmen2.fw";
			coreriver,sensing_ch_num = <0x4>;
			coreriver,tsk_ic_num = <0x1>; /* tc300k : 0x00, tc350k : 0x01 */
			coreriver,use_bitmap = <1>;
		};
	};

	i2c@25 {
		compatible = "i2c-gpio";
		gpios = <&gpd3 4 0
			&gpd3 3 0>;
		i2c-gpio,delay-us = <5>;
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";

		muic-s2mm001@25 {
			compatible = "sec-muic,i2c";
			reg = <0x25>;
			pinctrl-names = "default";
			pinctrl-0 = <&muic_irq>;
			muic,muic_int = <&gpa1 3 0>;
			gpios = <&gpd2 2 0>;
		};
	};

	i2c@26 {
		compatible = "i2c-gpio";
		gpios = <&gpb0 2 0
			&gpb0 3 0>;
		i2c-gpio,delay-us = <5>;
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";

		ktd2026@26 {
			compatible = "ktd2026,i2c";
			reg = <0x30>;
			pinctrl-names = "default";

			led_conf@1 {
				led_conf-name = "led_r";
				brightness = <0>;
				max_brightness = /bits/ 8 <0x20>;
				flags = <0>;
			};
			led_conf@2 {
				led_conf-name = "led_g";
				brightness = <0>;
				max_brightness = /bits/ 8 <0x20>;
				flags = <0>;
			};
			led_conf@3 {
				led_conf-name = "led_b";
				brightness = <0>;
				max_brightness = /bits/ 8 <0x20>;
				flags = <0>;
			};
		};
	};

	i2c@34 {
		compatible = "i2c-gpio";
		gpios = <&gpd2 1 0
			&gpd2 0 0>;
		i2c-gpio,delay-us = <5>;
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";

		s2mu003@34 {
			compatible = "samsung,s2mu003mfd";
			reg = <0x34>;
			pinctrl-names = "default";
			pinctrl-0 = <&if_irq>;
			s2mu003,irq-gpio = <&gpa0 5 0>;

			regulators {
				s2mu003-buck1 {
					regulator-name = "VDD_CAM_DVDD_1P2";
					regulator-min-microvolt = <1200000>;
					regulator-max-microvolt = <1200000>;
				};
				s2mu003-ldo1 {
					regulator-name = "VDD_CAM_AF_2P8";
					regulator-min-microvolt = <2800000>;
					regulator-max-microvolt = <2800000>;
				};
			};

			leds {
				torch-gpio = <&gpe0 1 0>;
				flash-gpio = <&gpe0 2 0>;

				status = "okay";
				s2mu003-leds1 {
					ledname = "leds-sec1";
					id = <0>;
					brightness = <0x17>;
					timeout = <0x0>;
					max-brightness = <0x15>;
				};

				s2mu003-leds2 {
					ledname = "leds-sec2";
					id = <1>;
					brightness = <0x02>;
					timeout = <0x0>;
					max-brightness = <0xf>;
				};
			};
		};
	};

	hsi2c@104c0000 {
		status = "okay";
		samsung,hs-mode;
		clock-frequency = <2500000>;
		samsung,hsi2c-batcher;
		#address-cells = <1>;
		#size-cells = <0>;

		audio_codec_cod3022x: cod3022x@04 {
			compatible = "codec,cod3022x";
			reg = <0x04>;
			ten-bit-address;
			vdd1-supply = <&ldo22_reg>;
			vdd2-supply = <&ldo23_reg>;
			pinctrl-names = "default";
			pinctrl-0 = <&cod3022_irq>;
			interrupt-parent = <&gpa0>;
			gpios = <&gpa0 4 0xf>;
			/* Mic Bias: 0->2.1v, 1->2.5v, 2->2.6v, 3->3.0v */
			mic-bias1-voltage = <3>;
			mic-bias2-voltage = <3>;
		};

		s2mpu03_pmic@66 {
